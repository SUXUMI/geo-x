შემოიღე სტანდარტები

ცარიელი ფუნქცია მთავარი ()
{
    რიცხვი ი;
    რიცხვი ჯ = 10;

    ამისთვის(ი = 0; ი < ჯ; ი++) 
    {
        დაბეჭდე("%d", ი);
    }

    სანამ(ი > 1) 
    {
    	ი--;
    	დაბეჭდე("%d", ი);
    }
}